                    <td class="contentTd" colspan="2"><INPUT NAME="PRtTax" TYPE="TEXT" value="0" style="width: 100px" readonly></td>
                    
                </tr>
                <tr>
                    <th class="contentTh BGC2 center" >文字</th>
                    <td class="contentTd" colspan="2"><INPUT NAME="EstTotal" TYPE="TEXT" value="0" style="width: 100px" readonly></td>
                    
                    <th class="contentTh BGC1 center" colspan="2">文字</th>
                    <td class="contentTd" colspan="2"><INPUT NAME="PRTotal" TYPE="TEXT" value="0" style="width: 100px" readonly></td>
                </tr>
                <tr>	
                    <th class="contentTh BGC1 center" rowspan="2" >文字</th>
                    
                    <th class="contentTh BGC1 center" colspan="2">文字</th>
                    <th class="contentTh BGC1 center" colspan="3">文字</th>
                    <th class="contentTh BGC1 center" colspan="4">文字</th>
                </tr>
                <tr>
                    <td class="contentTd" colspan="2"><INPUT NAME="FinTypeID" TYPE="TEXT"  >
                                                      <INPUT NAME="FinType" TYPE="TEXT" style="display:none" ></td>
                    <td class="contentTd" colspan="3"><INPUT NAME="RecomDeport" TYPE="TEXT"  >
