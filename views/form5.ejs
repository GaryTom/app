                </tr>
                <tr>
                    <th class="contentTh BGC1" >文字<font style="color:red" >*</font></th>
                    <td class="contentTd"  colspan="9"><INPUT NAME="PRItem" TYPE="TEXT"  ></td>
                </tr>
                <tr>
                    <th class="contentTh BGC1" >文字</th>
                    <td class="contentTd" colspan="5"><INPUT NAME="PRDate" TYPE="TEXT"  readonly></td>
            
                    <th class="contentTh BGC1" colspan="2">文字<font style="color:red">*</font></th>
                    <td class="contentTd" colspan="2"><INPUT NAME="POType" TYPE="TEXT" ></td>
                </tr>
                <tr>
                    <th class="contentTh BGC1" >文字<font style="color:red">*</font></th>
                    <td class="contentTd" colspan="9"><INPUT NAME="POFinishDate" TYPE="TEXT"  readonly></td>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th class="contentTh BGC2 center" colspan="6">文字</th>
                    <th class="contentTh BGC1 center" colspan="4">文字</th>
                </tr>
                <tr>
                    <th class="contentTh BGC2 center" >文字</th>
                    <th class="contentTh BGC2 center" >文字</th>
                    <th class="contentTh BGC2 center" >文字</th>
                    <th class="contentTh BGC2 center" >文字</th>
                    <th class="contentTh BGC2 center" >文字</th>
                    <th class="contentTh BGC2 center" >文字</th>
                    <th class="contentTh BGC1 center" >文字</th>
                    <th class="contentTh BGC1 center" >文字</th>
                    <th class="contentTh BGC1 center" >文字</th>
                    <th class="contentTh BGC1 center" >文字</th>
                </tr>
                <tr>
                    <td class="contentTd" ><INPUT NAME="ItemName_M" TYPE="TEXT" style="width: 110px" ></td>
                    <td class="contentTd" ><INPUT NAME="Standard_M" TYPE="TEXT" style="width: 110px" ></td>
                    <td class="contentTd" align="center"><INPUT NAME="Unit_M" TYPE="TEXT" style="width: 50px;text-align:center;" ></td>
                    <td class="contentTd" align="center"><INPUT NAME="EstAmount_M" TYPE="TEXT" style="width: 50px;text-align:center" ></td>
                    <td class="contentTd" ><INPUT NAME="EstUnit_M" TYPE="TEXT" style="width: 80px" ></td>
                    <td class="contentTd" ><INPUT NAME="EstFinishPrice_M" TYPE="TEXT" style="width:100px" value="0" readonly></td>
                    <td class="contentTd" align="center"><INPUT NAME="PO_Amount_M" TYPE="TEXT" style="width: 80px;text-align:center" ></td>
                    <td class="contentTd" ></td>
                    <td class="contentTd" ></td>
                    <td class="contentTd" ><INPUT NAME="PO_FinishPrice_M" TYPE="TEXT" style="width: 100px" value="0" readonly></td>
                </tr>
                <tr>
                    <th class="contentTh BGC1 center" rowspan="3" >文字</th>
                    
                    <th class="contentTh BGC1 center" >文字</th>
                    <td class="contentTd" ><INPUT NAME="SugTypeID" TYPE="TEXT" style="width: 110px" >
                                           <INPUT NAME="SugType" TYPE="TEXT" style="display:none" ></td>
                    
                    <th class="contentTh BGC2 center" >文字</th>
                    <td class="contentTd" colspan="2"><INPUT NAME="EstSubTotal" TYPE="TEXT" value="0"  style="width: 100px" readonly></td>
                    
                    <th class="contentTh BGC1 center" colspan="2">文字</th>
                    <td class="contentTd" colspan="2"><INPUT NAME="PRSubTotal" TYPE="TEXT" value="0"  style="width: 100px" readonly></td>
                </tr>
                <tr>
                    <th class="contentTh BGC1 center" rowspan="2">文字</th>
                    <td class="contentTd" rowspan="2"><INPUT NAME="RecomFirm" TYPE="TEXT" style="width: 100px" ></td>
                
                    <th class="contentTh BGC2 center" >文字</th>
                    <td class="contentTd" colspan="2"><INPUT NAME="EstTax" TYPE="TEXT" value="0" style="width: 100px" readonly></td>
                    
