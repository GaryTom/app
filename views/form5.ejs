                    <td class="contentTd" colspan="4"><INPUT NAME="RecomTranDeport" TYPE="TEXT" ></td>
                </tr>
            </tbody>
            <tfoot>
                <tr>
                    <th class="contentTh BGC1 center" >文字</th>
                    <td class="contentTd" colspan="9" ><INPUT NAME="PR_Note" TYPE="TEXT" ></td>									
                </tr>
                <tr>
                    <th class="contentTh BGC1 center" >文字</th>
                    <td class="contentTd" colspan="5"><INPUT NAME="PR_ManagerNote" TYPE="TEXT"  ></td>
                    
                    <th class="contentTh BGC1 center" >文字</th>
                    <td class="contentTd" colspan="5"><INPUT NAME="PR_ManagerSign" TYPE="TEXT"  value="" ></td>
                </tr>
                <tr>
                    <th class="contentTh BGC1 center" >文字</th>
                    <td class="contentTd" colspan="5"><INPUT NAME="PR_ChairNote" TYPE="TEXT"  ></td>
                    
                    <th class="contentTh BGC1 center" >文字</th>
                    <td class="contentTd" colspan="3"><INPUT NAME="PR_ChairSign" TYPE="TEXT" ></td>
                </tr>
            </tfoot>
        </table>
        <br>
    </div>
</body>
</html>